span className={className} onClick={() => handleSlide(props)}>\r\n        {char}\r\n      </span>\r\n    );\r\n  }\r\n\r\n  function customPaging(i) {\r\n    return <span className=\"test\">0{i + 1}</span>;\r\n  }\r\n\r\n  function appendDots(dots) {\r\n    return (\r\n      <div className=\"tech-paging\" onClick={handlePaging(dots)}>\r\n        <ul className=\"tech-paging__numb\"> {dots} </ul>\r\n      </div>\r\n    );\r\n  }\r\n\r\n  const renderSlides = () =>\r\n    carousel.map((num) => (\r\n      <div className=\"cus-slide\">\r\n        <div className=\"cus-slide__img\">\r\n          <img src={num.image} alt=\"\" />\r\n        </div>\r\n      </div>\r\n    ));\r\n\r\n  const renderText = () => (\r\n    <div>\r\n      <h4 className=\"technology__text--title-small\">{slideTitle}</h4>\r\n\r\n      <p className=\"technology__text--description\">{slideText}</p>\r\n    </div>\r\n  );\r\n\r\n  return (\r\n    <div className=\"technology\">\r\n      <div className=\"technology__img\">\r\n        <Slider\r\n          nextArrow={<Arrow type=\"next\" />}\r\n          prevArrow={<Arrow type=\"prev\" />}\r\n          dots={true}\r\n          customPaging={customPaging}\r\n          appendDots={appendDots}\r\n        >\r\n          {renderSlides()}\r\n        </Slider>\r\n      </div>\r\n\r\n      <div className=\"technology__text\">\r\n        <h2 className=\"technology__text--title-main\">công nghệ nổi bật</h2>\r\n        {renderText()}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Technology;\r\n","F:\\React-JS\\webchallenge\\frontend\\src\\components\\layout\\user\\homePage\\HomeNews.js",[],"F:\\React-JS\\webchallenge\\frontend\\src\\components\\layout\\user\\homePage\\BusinessProduct.js",[],"F:\\React-JS\\webchallenge\\frontend\\src\\components\\layout\\user\\homePage\\BusinessAreas.js",["255","256","257","258"],"import React from \"react\";\r\n\r\n//image\r\nimport ChuctetBG from \"../../../../assets/images/business-areas/chuctet.png\";\r\nimport Kha